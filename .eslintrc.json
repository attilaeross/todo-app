{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:jest/recommended",
    "plugin:jest/style",
    "plugin:jest-dom/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["jest", "jest-dom"],
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }]
  }
}
